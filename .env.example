# Mapbox API (required for maps and location search)
MAPBOX_API_KEY=your_mapbox_api_key_here
NEXT_PUBLIC_MAPBOX_API_KEY=your_mapbox_public_key_here

# PostHog Analytics (optional)
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key_here

# Resend Email Service (required for waitlist emails)
RESEND_API_KEY=your_resend_api_key_here

# OpenAI API (required for AI trip generation)
OPENAI_API_KEY=your_openai_api_key_here

# Google AI API (alternative to OpenAI)
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_key_here

# Vercel KV (required for caching)
KV_URL=your_vercel_kv_url_here
KV_REST_API_URL=your_vercel_kv_rest_url_here
KV_REST_API_TOKEN=your_vercel_kv_token_here
KV_REST_API_READ_ONLY_TOKEN=your_vercel_kv_readonly_token_here
